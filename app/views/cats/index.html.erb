<div class="container">
  <div class="row">
    <% @cats.each do |cat| %>
      <div class="col-xs-6 col-sm-4 col-lg-3">
        <div class="index-container">
          <%= image_tag cat.url, class:'index-img-container' %>
          <%= pluralize(cat.rate, 'like', 'likes') %>
        </div>
      </div>
    <% end %>
  </div>
</div>
